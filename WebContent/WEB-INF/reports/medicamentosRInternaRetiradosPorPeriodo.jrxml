	<parameter name="dtInicio" class="java.util.Date">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="dtFim" class="java.util.Date">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[select p.nome as medicamento,sum(r.quant),u.nome as ubs, u.localidade
from RECEITA r, PRODUTO p,AGENDA a, UBS u
where r.id_medicamento = p.id
and r.id_agenda = a.id
and a.id_ubs = u.id
and r.status = 'Concluído'
and r.dtencerramento between $P{dtInicio} and $P{dtFim}
group by p.nome
order by sum(r.quant) desc;]]>
	</queryString>
